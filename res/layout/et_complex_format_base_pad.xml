<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:background="@color/backgroundColor" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <cn.wps.moffice.spreadsheet.control.EtTitleBar android:id="@id/et_complex_format_title_bar" android:layout_width="fill_parent" android:layout_height="wrap_content" />
    <cn.wps.moffice.common.beans.SecondFullScreenLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <LinearLayout android:orientation="horizontal" android:background="@color/backgroundColor" android:layout_width="fill_parent" android:layout_height="fill_parent">
            <ScrollView android:layout_width="wrap_content" android:layout_height="fill_parent">
                <LinearLayout android:orientation="vertical" android:id="@id/et_complex_format_left" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <TextView android:id="@id/et_complex_format_numformat_btn" android:text="@string/et_toolbar_numformat" style="@style/ss_second_fullscreen_left_btn" />
                    <View style="@style/ss_second_fullscreen_left_divider" />
                    <TextView android:id="@id/et_complex_format_align_btn" android:text="@string/public_text_alignment" style="@style/ss_second_fullscreen_left_btn" />
                    <View style="@style/ss_second_fullscreen_left_divider" />
                    <TextView android:id="@id/et_complex_format_font_btn" android:text="@string/public_ribbon_font" style="@style/ss_second_fullscreen_left_btn" />
                    <View style="@style/ss_second_fullscreen_left_divider" />
                    <TextView android:id="@id/et_complex_format_frame_btn" android:text="@string/public_quickstyle_shape_outline" style="@style/ss_second_fullscreen_left_btn" />
                    <View style="@style/ss_second_fullscreen_left_divider" />
                    <TextView android:id="@id/et_complex_format_fill_cellse_btn" android:text="@string/public_quickstyle_shape_fill" style="@style/ss_second_fullscreen_left_btn" />
                    <View style="@style/ss_second_fullscreen_left_divider" />
                    <TextView android:id="@id/et_complex_format_protect_btn" android:text="@string/et_complex_format_protect" style="@style/ss_second_fullscreen_left_btn" />
                    <View style="@style/ss_second_fullscreen_left_divider" />
                </LinearLayout>
            </ScrollView>
            <View android:background="@color/lineColor" android:layout_width="1.0px" android:layout_height="fill_parent" />
            <LinearLayout android:orientation="vertical" android:id="@id/et_complex_format_main_content" android:background="@color/backgroundColor" android:layout_width="fill_parent" android:layout_height="fill_parent" />
        </LinearLayout>
    </cn.wps.moffice.common.beans.SecondFullScreenLayout>
</LinearLayout>