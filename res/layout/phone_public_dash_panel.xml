<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <View android:id="@id/dash_panel_background" android:layout_width="fill_parent" android:layout_height="fill_parent" />
    <FrameLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
        <View android:orientation="horizontal" android:id="@id/dash_space" android:visibility="invisible" android:clickable="true" android:layout_width="fill_parent" android:layout_height="fill_parent" />
        <FrameLayout android:layout_gravity="bottom" android:id="@id/dash_bar_layout" android:background="@color/phone_public_list_press_color_noalpha" android:layout_width="fill_parent" android:layout_height="@dimen/phone_public_dash_bar_height">
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@id/dash_bar" android:clickable="true" android:layout_width="fill_parent" android:layout_height="fill_parent" />
            <View android:layout_gravity="top" android:id="@id/dash_bar_line" style="@style/phone_title_split_line" />
        </FrameLayout>
        <FrameLayout android:layout_gravity="bottom" android:id="@id/dash_board" android:background="@color/secondBackgroundColor" android:layout_width="fill_parent" android:layout_height="wrap_content" style="@style/phone_document_actionbar_dash_board" />
    </FrameLayout>
</FrameLayout>