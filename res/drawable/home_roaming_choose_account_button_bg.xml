<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/buttonThirdPressedColor" />
            <stroke android:width="0.33000004dip" android:color="@color/subLineColor" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/thirdBackgroundColor" />
            <stroke android:width="0.33000004dip" android:color="@color/subLineColor" />
            <corners android:radius="4.0dip" />
        </shape>
    </item>
</selector>