<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:drawable="@drawable/public_rounded_rectangle_fill_8_shape" />
    <item android:state_hovered="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/largeIconHoverBackgroundColor" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
    <item android:state_selected="false" android:drawable="@android:color/transparent" />
    <item android:state_enabled="false" android:drawable="@color/disableColor" />
</selector>