<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="#fff76d70" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#77ffffff" android:endColor="#e1ffffff" android:angle="0.0" android:centerColor="#afffffff" />
            </shape>
        </clip>
    </item>
</layer-list>