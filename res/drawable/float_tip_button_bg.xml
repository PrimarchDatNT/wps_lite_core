<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/phone_public_shade_press"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape>
                    <solid android:color="@color/value_float_tip_blue_pressed" />
                    <corners android:radius="2.0dip" />
                </shape>
            </item>
            <item>
                <shape>
                    <solid android:color="@color/value_float_tip_blue_normal" />
                    <corners android:radius="2.0dip" />
                </shape>
            </item>
        </selector>
    </item>
</ripple>