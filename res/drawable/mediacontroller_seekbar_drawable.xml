<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1.0dip" />
            <stroke android:width="2.0dip" />
            <gradient android:startColor="#ff484848" android:endColor="#ff484848" android:angle="90.0" android:centerY="0.75" android:centerColor="#ff484848" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="1.0dip" />
                <stroke android:width="2.0dip" />
                <gradient android:startColor="#ff848484" android:endColor="#ff848484" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff848484" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1.0dip" />
                <stroke android:width="2.0dip" />
                <gradient android:startColor="#ffec700c" android:endColor="#ffec700c" android:angle="90.0" android:centerY="0.75" android:centerColor="#ffec700c" />
            </shape>
        </clip>
    </item>
</layer-list>