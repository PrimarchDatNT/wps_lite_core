<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/rippleColor"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_pressed="true" android:drawable="@drawable/phone_public_divideview_bgcolor_hi" />
            <item android:state_selected="true">
                <color android:color="@color/WPSMainColor" />
            </item>
            <item android:state_enabled="false">
                <color android:color="@drawable/color_alpha_disable" />
            </item>
            <item android:drawable="@android:color/transparent" />
        </selector>
    </item>
</ripple>