<?xml version="1.0" encoding="utf-8"?>
<layer-list android:layout_width="3.5dip" android:layout_height="wrap_content"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:layout_width="wrap_content">
        <shape>
            <corners android:radius="2.0dip" />
            <solid android:color="#1a000000" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#ff417ff9" android:endColor="#ff417ff9" android:angle="90.0" android:centerColor="#ff417ff9" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <solid android:color="#ff417ff9" />
            </shape>
        </clip>
    </item>
</layer-list>