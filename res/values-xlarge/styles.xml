<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PptTheme" parent="@style/OfficeAppTheme">
        <item name="android:textAppearance">@style/PptDefaultText</item>
        <item name="android:windowBackground">@color/backgroundColor</item>
        <item name="android:checkboxStyle">@style/PptPhoneCheckBox</item>
        <item name="android:editTextStyle">@style/PptPhoneEdittext</item>
        <item name="android:progressBarStyle">@style/PptProgressbar</item>
        <item name="android:progressBarStyleHorizontal">@style/PptPhoneProgressBar.Horizontal</item>
        <item name="android:seekBarStyle">@style/PptPhoneSeekbarStyle</item>
        <item name="android:radioButtonStyle">@style/PptPhoneRadioButton</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:progressBarStyleLargeInverse">@style/PptPhoneCirclProgressBarLarge</item>
        <item name="activity_theme_Style">@style/PptPhoneThemeStyle</item>
        <item name="activity_theme_color">@color/WPPMainColor</item>
        <item name="my_autoCompleteTextViewStyle">@style/Pptphone_autocomplete</item>
    </style>
    <style name="documents_file_item_wrapstyle">
        <item name="android:paddingLeft">25.0dip</item>
        <item name="android:paddingTop">@dimen/documents_file_item_wrapstyle_paddingtop</item>
        <item name="android:paddingRight">0.0dip</item>
        <item name="android:paddingBottom">@dimen/documents_file_item_wrapstyle_paddingbottom</item>
        <item name="android:descendantFocusability">blocksDescendants</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">25.0dip</item>
        <item name="android:paddingEnd">0.0dip</item>
    </style>
    <style name="documents_history_delete_undo">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">bottom|center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/documents_history_delete_undo_background</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">450.0dip</item>
        <item name="android:layout_height">45.0dip</item>
        <item name="android:layout_marginBottom">@dimen/docemtnts_history_delete_undo_bottom_padding</item>
    </style>
    <style name="documents_history_record_item_color_point">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">39.0dip</item>
        <item name="android:layout_marginTop">30.0dip</item>
        <item name="android:layout_marginRight">2.0dip</item>
        <item name="android:layout_marginStart">39.0dip</item>
        <item name="android:layout_marginEnd">2.0dip</item>
    </style>
    <style name="documents_history_record_summary_file_name">
        <item name="android:maxEms">12</item>
    </style>
    <style name="documents_operation_layout">
        <item name="android:gravity">bottom</item>
        <item name="android:layout_gravity">bottom|center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">162.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/docemtnts_history_document_operation_layout_bottom_padding</item>
        <item name="android:baselineAligned">true</item>
    </style>
    <style name="et_prot_sheet_opt" parent="@style/public_image_bg_style">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/mainTextColor</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">40.0dip</item>
        <item name="android:paddingRight">40.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">72.0dip</item>
        <item name="android:checkMark">@drawable/phone_public_checkbox_selector</item>
        <item name="android:paddingStart">40.0dip</item>
        <item name="android:paddingEnd">40.0dip</item>
    </style>
    <style name="et_prot_sheet_opt_headline">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">40.0dip</item>
        <item name="android:layout_marginTop">20.0dip</item>
        <item name="android:layout_marginRight">40.0dip</item>
        <item name="android:layout_marginBottom">20.0dip</item>
        <item name="android:layout_marginStart">40.0dip</item>
        <item name="android:layout_marginEnd">40.0dip</item>
    </style>
    <style name="phone_ss_seekbar_style" parent="@style/public_seekbar_base">
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingRight">5.0dip</item>
        <item name="android:focusable">true</item>
        <item name="android:maxHeight">3.0dip</item>
        <item name="android:progressDrawable">@drawable/phone_ss_seekbar_progress_bar</item>
        <item name="android:minHeight">3.0dip</item>
        <item name="android:thumb">@drawable/phone_public_seekbar_thumb_selector</item>
        <item name="android:thumbOffset">15.0dip</item>
        <item name="android:paddingStart">5.0dip</item>
        <item name="android:paddingEnd">5.0dip</item>
    </style>
    <style name="public_app_btn">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:scaleType">fitXY</item>
    </style>
</resources>
